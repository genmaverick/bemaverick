<?xml version="1.0" encoding="utf-8"?>
<docs>
    <name>Auth Reset Password</name>
    <service_name>authResetPassword</service_name>
    <description>Reset the password for a user</description>
    <uri>/auth/resetpassword</uri>
    <method>POST</method>
    <parameters>
        <param>
            <name>appKey</name>
            <required>yes</required>
            <default></default>
            <format>string</format>
            <examples>qiusdjsls9273sd</examples>
            <description>Your application key</description>
        </param>
        <param>
            <name>code</name>
            <required>yes</required>
            <default></default>
            <format>string</format>
            <examples></examples>
            <description>The code that was given to the user via email to reset their password</description>
        </param>
        <param>
            <name>newPassword</name>
            <required>yes</required>
            <default></default>
            <format>string</format>
            <examples>mypassword</examples>
            <description>The new password for the user</description>
        </param>
    </parameters>
    <example_request><![CDATA[/auth/resetpassword?code=jskj28djs,djs823&newPassword=mypassword&appKey=test_key]]></example_request>
</docs>
