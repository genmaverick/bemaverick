<?xml version="1.0" encoding="utf-8"?>
<docs>
    <name>Site Config</name>
    <description>Get the config for entire site</description>
    <uri>/site/config</uri>
    <method>GET</method>
    <parameters>
        <param>
            <name>appKey</name>
            <required>yes</required>
            <default></default>
            <format>string</format>
            <examples>test_key</examples>
            <description>Your application key</description>
        </param>
    </parameters>

    <example_curl><![CDATA[
curl --request GET \
  --header "Authorization: Bearer <accessToken>" \
  --url "<host>/v1/site/config?appKey=test_key"
]]></example_curl>

