<?xml version="1.0" encoding="utf-8"?>
<docs>
    <name>User Saved Challenges</name>
    <description>Get the list of saved challenges for the user.  Ordered by most recently updated, first.</description>
    <uri>/user/savedchallenges</uri>
    <method>GET</method>
    <parameters>
        <param>
            <name>appKey</name>
            <required>yes</required>
            <default></default>
            <format>string</format>
            <examples>qiusdjsls9273sd</examples>
            <description>Your application key</description>
        </param>
    </parameters>

    <example_curl><![CDATA[
curl --request GET \
  --header "Authorization: Bearer <accessToken>" \
  --url "<host>/v1/user/savedchallenges?appKey=test_key"
]]></example_curl>

    <example_json_response><![CDATA[
