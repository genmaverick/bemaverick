<?xml version="1.0" encoding="utf-8"?>
<docs>
    <name>Site Maverick Stream</name>
    <description>Get the list of responses and challenges in the main maverick stream</description>
    <uri>/site/maverickstream</uri>
    <method>GET</method>
    <parameters>
        <param>
            <name>appKey</name>
            <required>yes</required>
            <default></default>
            <format>string</format>
            <examples>test_key</examples>
            <description>Your application key</description>
        </param>
        <param>
            <name>count</name>
            <required>no</required>
            <default>25</default>
            <format>integer</format>
            <examples>25, 50</examples>
            <description>Return the number of objects</description>
        </param>
        <param>
            <name>offset</name>
            <required>no</required>
            <default>0</default>
            <format>integer</format>
            <examples>10</examples>
            <description>Start with the offset for list of objects. 0 based.</description>
        </param>
    </parameters>

    <example_curl><![CDATA[
curl --request GET \
  --header "Authorization: Bearer <accessToken>" \
  --url "<host>/v1/site/maverickstream?appKey=test_key"
]]></example_curl>

    <example_json_response><![CDATA[
{
  "TODO": "TODO"
}
]]></example_json_response>

</docs>
