<?xml version="1.0" encoding="utf-8"?>
<docs>
    <name>Response Share</name>
    <description>Share a response by making it public and getting a share url back</description>
    <uri>/response/share</uri>
    <method>POST</method>
    <parameters>
        <param>
            <name>appKey</name>
            <required>yes</required>
            <default></default>
            <format>string</format>
            <examples>qiusdjsls9273sd</examples>
            <description>Your application key</description>
        </param>
        <param>
            <name>responseId</name>
            <required>yes</required>
            <default></default>
            <format>integer</format>
            <examples>1, 542</examples>
            <description>The response id</description>
        </param>
    </parameters>

    <example_curl><![CDATA[
curl --request POST \
  --header "Authorization: Bearer <accessToken>" \
  --url "<host>/v1/response/share" \
  --data "responseId=1&appKey=test_key"
]]></example_curl>

    <example_json_response><![CDATA[
]]>
    </example_json_response>

</docs>
