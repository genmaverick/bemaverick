<?xml version="1.0" encoding="utf-8"?>
<docs>
    <name>Challenge Delete</name>
    <description>Delete a challenge</description>
    <uri>/challenge/delete</uri>
    <method>POST</method>
    <parameters>
        <param>
            <name>appKey</name>
            <required>yes</required>
            <default></default>
            <format>string</format>
            <examples>qiusdjsls9273sd</examples>
            <description>Your application key</description>
        </param>
        <param>
            <name>challengeId</name>
            <required>yes</required>
            <default></default>
            <format>integer</format>
            <examples>1, 542</examples>
            <description>The challenge id to edit</description>
        </param>
    </parameters>

    <example_curl><![CDATA[
curl --request POST \
  --header "Authorization: Bearer <accessToken>" \
  --url "<host>/v1/challenge/delete" \
  --data "challenge=156&appKey=test_key"
]]></example_curl>

