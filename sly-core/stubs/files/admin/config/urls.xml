<?xml version="1.0" encoding="utf-8"?>
<urls>

    <imageStatic>
        <url>/cache/img/:imageName</url>
        <params>
            <param required="true" urlencode="false">imageName</param>
        </params>
    </imageStatic>

    <cssStatic>
        <url>/cache/css/:maxModTime/:cssFiles.css</url>
        <params>
            <param required="true" urlencode="false">maxModTime</param>
            <param required="true" urlencode="false">cssFiles</param>
        </params>
    </cssStatic>

    <jsStatic>
        <url>/cache/js/:maxModTime/:jsFiles.js</url>
        <params>
           <param required="true" urlencode="false">maxModTime</param>
           <param required="true" urlencode="false">jsFiles</param>
        </params>
    </jsStatic>

    <fontStatic>
        <url>/cache/fonts/:fontName</url>
        <params>
            <param required="true" urlencode="false">fontName</param>
        </params>
    </fontStatic>

    <home>
        <url>/</url>
    </home>

</urls>
